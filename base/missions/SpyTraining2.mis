function SpyTraining2::purge(%this, %player) {
  if (getObjectType(%player) != "Player") return;

  if (%player.grappling) Grappler::ungrapple(%player);

  Player::setItemCount(%player, Grappler, 0);

  Player::unmountItem(%player, 0);
}

/////
// GRAPPLER TRAINING TRIGGER FUNCTIONS
/////

function GrapplerTraining::enter(%this, %player) {
  if (getObjectType(%player) != "Player") return;
  Player::setItemCount(%player, Grappler, 1);
  Player::mountItem(%player, Grappler, 0);

  centerprint(Player::getClient(%player), "<L5><JC><F1>Welcome to Grappler Training!<F0>\n\nIf you are a <F1>Grappling beginner<F0>, turn right and step onto the <F1>glowing tile<F0> to get started.\n\nTouch the tower switch for more info.", 1000);
}

function GrapplerTraining::leave(%this, %player) {
  if (getObjectType(%player) != "Player") return;
  centerprint(Player::getClient(%player), "", 1);
}

function BeginnerTraining1::enter(%this, %player) {
  if (getObjectType(%player) != "Player") return;

  if (Player::getMountedItem(%player, 0) != "Grappler" || !((%player.beginnerTrainingStep >= 1 && %player.beginnerTrainingStep <= 5) || %player.beginnerTrainingStep == 9)) {
    %client = Player::getClient(%player);
    centerprint(%client, "<JC><L1>\nHiya! Let's get started. First, to play SpyMod, you MUST set your binds for 1-5 back to their " @
                         "normal functions! If you need to do this, hit Tab, click Help, then \"How to Reset Binds\" for help. " @
                         "Then come back and press <F1>\"2\"<F0> ingame.\n\n", 1000);

    %player.beginnerTrainingStep = 1;
  }

  if (%player.beginnerTrainingStep == 9) {
    %client = Player::getClient(%player);
    centerprint(%client, "<JC><L1>\n<F1>Congratulations!<F0>\n\nYou've just completed the Beginner's Grappler Training.\n\nNow, you should learn how to use Expert Mode, which combines the Grapple, Ascend, and Descend modes all into one. Hit Tab, Help, Training, then Grappler Training for help.\n\n", 1000);

    %player.beginnerTrainingStep = 10;
  }
}

function BeginnerTraining1::contact(%this, %player) {
  if (getObjectType(%player) != "Player") return;
  if (%player.beginnerTrainingStep == 1 && Player::getMountedItem(%player, 0) == "Grappler") {
    if (%player.trainingCharge < 7) { %player.trainingCharge++; return; }

    %client = Player::getClient(%player);
    centerprint(%client, "<JC><L1>\nOk, now, change to <F1>Grapple mode<F0> by pressing the <F1>\"3\"<F0> key until it says <F1>\"Grapple Mode\"<F0>. While in Grapple Mode, you can <F1>fire to set the Grappler hook<F0>, then <F1>fire again to unset it<F0> (let go).\n\n", 1000);

    %player.beginnerTrainingStep = 2;
    %player.trainingCharge = 0;
  }

  if (%player.beginnerTrainingStep == 2 && Player::getMountedItem(%player, 0) == "Grappler" && Weapon::getMode(%player, "Grappler") == 0) {
    if (%player.trainingCharge < 7) { %player.trainingCharge++; return; }
    %client = Player::getClient(%player);
    centerprint(%client, "<JC><L1>\nAlright! Now, aim at the <F1>glowing circle<F0> on the ceiling directly above you, <F1>and fire at it<F0>. This will set the Grappler hook.\n\n", 1000);

    %player.beginnerTrainingStep = 3;
    %player.trainingCharge = 0;
  }

  if (%player.beginnerTrainingStep == 3 && %player.grappling) {
    if (%player.trainingCharge < 7) { %player.trainingCharge++; return; }
    %client = Player::getClient(%player);
    centerprint(%client, "<JC><L1>\nGreat job! Now, press the <F1>\"3\"<F0> key a again, so it says <F1>\"Ascend Mode\"<F0>. If you fire and hold while in this mode, you will reel in the Grappler rope, and thus ascend.\n\n", 1000);

    %player.beginnerTrainingStep = 4;
    %player.trainingCharge = 0;
  }

  if (%player.beginnerTrainingStep == 4 && Weapon::getMode(%player, "Grappler") == 1) {
    if (%player.trainingCharge < 7) { %player.trainingCharge++; return; }
    %client = Player::getClient(%player);
    centerprint(%client, "<JC><L1>\nThere you go! Now, <F1>fire and hold<F0> and you will <F1>ascend<F0>. Ascend until you are prompted with the next message.\n\n", 1000);

    %player.beginnerTrainingStep = 5;
    %player.trainingCharge = 0;
  }
}

function BeginnerTraining1::leave(%this, %player) {
  if (getObjectType(%player) != "Player") return;

  if (%player.beginnerTrainingStep == 10) %player.beginnerTrainingStep = "";
  else centerprint(Player::getClient(%player), "", 3);
}

function BeginnerTraining2::enter(%this, %player) {
  if (getObjectType(%player) != "Player") return;

  if (%player.beginnerTrainingStep == 5) {
    %client = Player::getClient(%player);
    centerprint(%client, "<JC><L1>\nNice job! Now, hit <F1>\"3\"<F0> a couple of times until you get to <F1>\"Swing Mode\"<F0>. While you are in this mode, you can <F1>fire and hold to swing forward<F0>.\n\n", 1000);

    %player.beginnerTrainingStep = 6;
  }
}

function BeginnerTraining2::contact(%this, %player) {
  if (getObjectType(%player) != "Player") return;

  if (%player.beginnerTrainingStep == 6 && Weapon::getMode(%player, Grappler) == 3) {
    if (%player.trainingCharge < 7) { %player.trainingCharge++; return; }

    %client = Player::getClient(%player);
    centerprint(%client, "<JC><L1>\nGreat! Now, alternate holding the fire button to swing forward and releasing it to fall back, then switch to <F1>Grapple Mode<F0> again and <F1>fire when you are over the platform<F0> to let go.\n\n", 1000);

    %player.beginnerTrainingStep = 7;
    %player.trainingCharge = 0;
  }
}

function BeginnerTraining2::leave(%this, %player) {
  if (getObjectType(%player) != "Player") return;
  centerprint(Player::getClient(%player), "", 3);
}

function BeginnerTraining3::contact(%this, %player) {
  if (getObjectType(%player) != "Player") return;

  if (%player.beginnerTrainingStep == 7 && !%player.grappling && Player::getLastContactCount(%player) == 0) {
    if (%player.grappling) Grappler::ungrapple(%player);
    %client = Player::getClient(%player);
    centerprint(%client, "<JC><L1>\nOk, now it's time to get back down. Change back to Grapple Mode, and fire at the glowing circle on the ceiling again. Then jump off the platform.\n\n", 1000);

    %player.beginnerTrainingStep = 8;
  }
}
function BeginnerTraining3::leave(%this, %player) {
  if (getObjectType(%player) != "Player") return;
  if (%player.beginnerTrainingStep == 8) centerprint(Player::getClient(%player), "", 3);
}

function BeginnerTraining4::contact(%this, %player) {
  if (getObjectType(%player) != "Player") return;

  if (%player.beginnerTrainingStep == 8 && %player.grappling) {
    %client = Player::getClient(%player);
    centerprint(%client, "<JC><L1>\nOk, now, to go down, select <F1>\"Descend Mode\"<F0>, and then <F1>fire and hold to go down<F0>. Walk onto the <F1>glowing tile<F0> once you're on the ground.\n\n", 1000);

    %player.beginnerTrainingStep = 9;
  }
}

function SwingTraining1::enter(%this, %player) {
  if (getObjectType(%player) != "Player") return;

  %time = getSimTime();

  if (%player.swingTrainingSide == 2 && %time - %player.swingTrainingSwingStart < 8) {
    centerprint(Player::getClient(%player), "<JC>\nGood swing!\n\n", 3);
  } else {
    %player.swingTrainingSide = 1;
    centerprint(Player::getClient(%player), "<JC>\nSee if you can swing to the other side! Grapple onto the round object in the center of the ceiling and swing!\n\n", 3);
  }
}

function SwingTraining1::leave(%this, %player) {
  if (getObjectType(%player) != "Player") return;

  %time = getSimTime();

  %player.swingTrainingSwingStart = %time;
  %player.swingTrainingSide = 1;
}

