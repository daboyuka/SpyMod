function PlayerTarget::onDamage(%this,%type,%value,%pos,%vec,%mom,%object) {
  if (%this.poppedUp) {
    GameBase::playSound(%this, SoundHitPlayerTarget, 0);
    %this.popUpID++;
    %this.poppedUp = false;
    Moveable::moveBackward(%this);
  }
}

function FiringRange::unpopupAll(%stall) {
  %g = nameToID("MissionGroup\\FiringRange\\SecondFloorTargets\\StallTargets"@%stall);
  %n = Group::objectCount(%g);
  for (%i = 0; %i < %n; %i++) {
    %targG = Group::getObject(%g, %i);
    %n2 = Group::objectCount(%targG);
    for (%j = 0; %j < %n2; %j++) {
      %m = Group::getObject(%targG, %j);
      if (GameBase::getDataName(%m).className == "target") Target::unpopup(%m);
    }
  }
}

function FiringRange::outOfBounds(%this, %object) {
  if (getObjectType(%object) != "Player") return;

  %curPos = GameBase::getPosition(%object);
  if (getWord(%curPos, 2) > 127) {
    if (%object.stall && getSimTime() - %object.stallLeaveTime < 2) %pos = (3+%object.stall*14) @ " 44.5 128";
    else %pos = "20 37 128";
  } else {
    %pos = getWord(%curPos, 0) @ " 45 " @ getWord(%curPos, 2);
  }

  GameBase::setPosition(%object, %pos);
  Item::setVelocity(%object, "0 0 0");
  Client::sendMessage(Player::getClient(%object), 1, "Stay behind the wall!");
}

function FiringRange::enterStall(%this, %object) {
  if (getObjectType(%object) != "Player") return;

  %time = getSimTime();

  if (%time - %object.stallLeaveTime > 2 || %object.stallLeaveTime == "") {
    centerprint(Player::getClient(%this), "<JC>Touch the panel on the left wall to start target practice.");
  }

  %object.stall = %this.stall;
  %object.stallLeaveTime = "";
}

function FiringRange::leaveStall(%this, %object) {
  if (getObjectType(%object) != "Player") return;

  %object.stallLeaveTime = getSimTime();
}

function StallPanel::onCollision(%this, %object) {
  if (getObjectType(%object) != "Player") return;

  if (%object.targetPractice) {
    %object.targetPractice = "";
    FiringRange::unpopupAll(%this.stall);
    centerprint(Player::getClient(%object), "<JC>Target practice cancelled.");
  } else {
    FiringRange::targetPracticeMenu(%object);
  }
}

function FiringRange::targetPracticeMenu(%object) {

}

