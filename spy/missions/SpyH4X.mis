$h4x = false;
function FinalPanel::onCollision(%this, %object) {
  if (%this.w00t >= 30) return;

  if (getObjectType(%object) != "Player") return;
  if (Player::isDead(%object) || GameBase::getTeam(%object) != 0) return;

  %c = Player::getClient(%object);

  %this.w00t++;

  if (%this.w00t == 1) Client::sendMessage(%c, 1, "Hacking into main database. Keep pushing into the panel...~wLeftMissionArea.wav");
  else                 {
    Client::sendMessage(%c, 1, "~wLeftMissionArea.wav");
    %s = "";
    for (%i = 0; %i < 30; %i++) %s = %s @ tern(%i < %this.w00t, "|", ".");
    bottomprint(%c, "<jc><f1>"@%s, 2); 
  }

  if (%this.w00t == 30) {
    $h4x = true;
    $teamScore[0] = $teamScoreLimit;
    teamMessages(1, 0, "Security of main database has been compromised.", 1, "Main database security successfully breached.");
    ObjectiveMission::checkScoreLimit();
  } else schedule("if ("@%this@".w00t=="@%this.w00t@")"@%this@".w00t=0;", 3);
}

