function FinalPanel::getObjectiveString(%this, %forTeam) {
  if ($missionComplete) {
    if (%forTeam == -1) {
      if ($h4x)
        return "<Btowers_neutral.bmp>\nThe main database's security was compromised by team " @ getTeamName(0) @ ".";
      else
        return "<Btowers_neutral.bmp>\nThe main database's security was not compromised by team " @ getTeamName(0) @ ".";
    } else if (%forTeam == 1) {
      if ($h4x)
        return "<Btower_teamcontrol.bmp>\nYour team failed prevent the main database's security from being compromised.";
      else
        return "<Btower_teamcontrol.bmp>\nYour team successfully prevent the main database's security from being compromised.";
    } else if (%forTeam == 0) {
      if ($h4x)
        return "<Btower_teamcontrol.bmp>\nYour team successfully hacked into the main database.";
      else
        return "<Btower_teamcontrol.bmp>\nYour team failed to hack into the main database.";
    }
  } else {
    if (%forTeam == -1) {
      if ($h4x)
        return "<Btowers_neutral.bmp>\nThe main database's security has been compromised by team " @ getTeamName(0) @ ".";
      else
        return "<Btowers_neutral.bmp>\nTeam " @ getTeamName(1) @ " must prevent team " @ getTeamName(0) @ " from hacking into the main database.";
    } else if (%forTeam == 1) {
      if ($h4x) {
        return "<Btower_teamcontrol.bmp>\nYour team failed prevent the main database's security from being compromised.";
      } else {
        return "<Btower_teamcontrol.bmp>\nProtect the main database and prevent it's security from being compromised.";
      }
    } else if (%forTeam == 0) {
      if ($h4x) {
        return "<Btower_teamcontrol.bmp>\nYour team successfully hacked into the main database.";
      } else {
        return "<Btower_teamcontrol.bmp>\nSuccessfully hack into the main database to win the mission.";
      }
    }
  }
}





$lockingDown = false;
$lockedDown = false;
function lockdown(%c) {
  if ($lockingDown) {
    return;
  }

  $lockingDown = true;

  %g = nameToID("MissionGroup\\LockdownDoors");
  %n = Group::objectCount(%g);
  %mt = 0;
  for (%i = 0; %i < %n; %i++) {
    %o = nameToID(Group::getObject(%g, %i) @ "\\ElevDoor");
    schedule("ElevDoor::open("@%o@");", %o.lockdownWait);
    if (%mt < %o.lockdownWait) %mt = %o.lockdownWait;
  }

  messageAll(1, "WARNING - BASE LOCKDOWN IN PROGRESS...~wfloat_target.wav");

  schedule("$lockingDown = false; $lockedDown = true;", %mt + 3);
}

function unlockdown(%c) {
  if ($lockingDown) {
    return;
  }

  $lockingDown = true;

  %g = nameToID("MissionGroup\\LockdownDoors");
  %n = Group::objectCount(%g);
  %mt = 0;
  for (%i = 0; %i < %n; %i++) {
    %o = nameToID(Group::getObject(%g, %i) @ "\\ElevDoor");
    schedule("ElevDoor::close("@%o@");", %o.unlockdownWait);
    if (%mt < %o.unlockdownWait) %mt = %o.unlockdownWait;
  }

  messageAll(1, "RELEASING LOCKDOWN DOORS...");

  schedule("$lockingDown = false; $lockedDown = false;", %mt + 3);
}

function LockdownPanel::onCollision(%this, %object) {
  if (getObjectType(%object) != "Player") return;
  if (Player::isDead(%object)) return;

  %c = Player::getClient(%object);

  if ($lockingDown) {
    Client::sendMessage(%c, 1, "Lockdown operation already in progress.");
    return;
  }

  %this.w00t += tern($lockedDown, 4, 1);

  if (%this.w00t == tern($lockedDown, 4, 1)) {
    if ($lockedDown) Client::sendMessage(%c, 1, "Preparing to release lockdown doors. Keep pushing into the panel...~wLeftMissionArea.wav");
    else Client::sendMessage(%c, 1, "Preparing for base lockdown. Keep pushing into the panel...~wLeftMissionArea.wav");
  } else {
    Client::sendMessage(%c, 1, "~wLeftMissionArea.wav");
    %s = "";
    for (%i = 0; %i < 20; %i++) %s = %s @ tern(%i < %this.w00t, "|", ".");
    bottomprint(%c, "<jc><f1>"@%s, 2); 
  }

  if (%this.w00t == 20) {
    if ($lockedDown) unlockdown(Player::getClient(%object));
    else             lockdown(Player::getClient(%object));
    %this.w00t = 0;
  } else schedule("if ("@%this@".w00t=="@%this.w00t@")"@%this@".w00t=0;", 3);
}

$finalDoorLock = 4;
function FinalDoorPanel::onCollision(%this, %object) {
  if (%this.w00t >= 8 || $finalDoorLock == 0) return;

  if (getObjectType(%object) != "Player" || GameBase::getTeam(%object) != 0) return;
  if (Player::isDead(%object)) return;

  %c = Player::getClient(%object);

  %this.w00t++;

  if (%this.w00t == 1) Client::sendMessage(%c, 1, "Unlocking main database vault door. Keep pushing into the panel...~wLeftMissionArea.wav");
  else                 {
    Client::sendMessage(%c, 1, "~wLeftMissionArea.wav");
    %s = "";
    for (%i = 0; %i < 8; %i++) %s = %s @ tern(%i < %this.w00t, "|", ".");
    bottomprint(%c, "<jc><f1>"@%s, 2); 
  }

  if (%this.w00t == 8) {
    $finalDoorLock--;
    if ($finalDoorLock == 0) {
      Client::sendMessage(%c, 1, "Vault door successfully unlocked.");
      ElevDoor::open(nameToID("MissionGroup\\MoreBigBase\\FinalDoor\\FinalDoor"));
    } else {
      Client::sendMessage(%c, 1, $finalDoorLock @ " locks remaining. Complete unlock at the other control panels.");
    }
  } else schedule("if ("@%this@".w00t=="@%this.w00t@")"@%this@".w00t=0;", 3);
}

function FinalDoorPanel2::onCollision(%this, %object) {
  if (%this.w00t >= 30 || $finalDoorLock == 0) return;

