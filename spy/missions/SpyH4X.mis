$ElevatorBox::group = nameToID("MissionGroup\\ElevatorBoxes");
$ElevatorBox::dest = -1;
$ElevatorBox::lastDest = -1;
function ElevatorBox::request(%object, %boxNum1, %boxNum2) {
  if (getObjectType(%object) != "Player" || Player::isDead(%object)) return;
  %client = Player::getClient(%object);

  if ($ElevatorBox::dest != -1) {
    Client::sendMessage(%client, 1, "Elevator is already running, please wait.");
    return;
  }

  %elevBox1 = nameToID($ElevatorBox::group @ "\\Box" @ %boxNum1);
  %elevBox2 = nameToID($ElevatorBox::group @ "\\Box" @ %boxNum2);
  %door1 = nameToID(%elevBox1 @ "\\Door\\Door");
  %door2 = nameToID(%elevBox2 @ "\\Door\\Door");

  if (!GameBase::isActive(%door1) || !GameBase::isActive(%door2)) {
    Client::sendMessage(%client, 1, "Elevator is not operational.");
    return;
  }

  if ($ElevatorBox::lastDest == -1 || $ElevatorBox::lastDest == %boxNum1) {
    echo("OPEN ELEV");
    ElevDoor::openAndClose(%door1, 5);
    schedule("ElevatorBox::tele("@%boxNum1@","@%boxNum2@");", 10);
    schedule("ElevDoor::open("@%door2@");", 12);
    schedule("$ElevatorBox::dest = -1;", 14);

    Client::sendMessage(%client, 1, "Elevator ready, please enter.");

    $ElevatorBox::dest = %boxNum2;
    $ElevatorBox::lastDest = %boxNum2;
  } else {
    echo("CLOSED ELEV");

    %boxNum3 = $ElevatorBox::lastDest;
    %elevBox3 = nameToID($ElevatorBox::group @ "\\Box" @ %boxNum3);
    %door3 = nameToID(%elevBox3 @ "\\Door\\Door");

    ElevDoor::close(%door3);
    schedule("ElevatorBox::tele("@%boxNum3@","@%boxNum1@");", 5);
    schedule("ElevDoor::openAndClose("@%door1@", 5);", 7);
    schedule("ElevatorBox::tele("@%boxNum1@","@%boxNum2@");", 17);
    schedule("ElevDoor::open("@%door2@");", 19);
    schedule("$ElevatorBox::dest = -1;", 21);

    Client::sendMessage(%client, 1, "Elevator ready, please enter.");

    $ElevatorBox::dest = %boxNum2;
    $ElevatorBox::lastDest = %boxNum2;
  }
}

function ElevatorBox::tele(%boxNum1, %boxNum2) {
  %elevBox1 = nameToID($ElevatorBox::group @ "\\Box" @ %boxNum1);
  %elevBox2 = nameToID($ElevatorBox::group @ "\\Box" @ %boxNum2);

  %marker = nameToID(%elevBox1 @ "\\Marker");
  %marker2 = nameToID(%elevBox2 @ "\\Marker");

  %set = newObject("", SimSet);

  containerBoxFillSet(%set, $SimPlayerObjectType | $MineObjectType | $ItemObjectType, GameBase::GetPosition(%marker), %marker.width, %marker.length, %marker.height, 0);

  %offset = Vector::sub(GameBase::getPosition(%marker2), GameBase::getPosition(%marker));

  %n = Group::objectCount(%set);
  for (%i = 0; %i < %n; %i++) {
    %o = Group::getObject(%set, %i);
    %p = GameBase::getPosition(%o);
    if (%o.grappling) Grappler::ungrapple(%o);
    GameBase::setPosition(%o, Vector::add(%p, %offset));
  }
  GameBase::playSound(%door1, SoundElevatorStart, 0);
  GameBase::playSound(%door2, SoundElevatorStart, 0);

  deleteObject(%set);
}

