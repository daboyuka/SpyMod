function H4XPanel::onCollision(%this, %object) {
  if (getObjectType(%object) != "Player" || GameBase::getTeam(%object) != 0) return;
  if (Player::isDead(%object)) return;

  %c = Player::getClient(%object);

  if ($lockingDown) {
    Client::sendMessage(%c, 1, "Lockdown operation is in progress; you must wait until it completes.");
    return;
  }

  if (!$lockedDown) {
    Client::sendMessage(%c, 1, "Cannot override lockdown since base is not currently locked down.");
    return;
  }

  %this.w00t++;

  if (%this.w00t == 1) {
    Client::sendMessage(%c, 1, "Attempting to override base lockdown. Keep pushing into the panel...~wLeftMissionArea.wav");
  } else {
    Client::sendMessage(%c, 1, "~wLeftMissionArea.wav");
    %s = "";
    for (%i = 0; %i < 20; %i++) %s = %s @ tern(%i < %this.w00t, "|", ".");
    bottomprint(%c, "<jc><f1>"@%s, 2); 
  }

  if (%this.w00t == 20) {
    unlockdown(Player::getClient(%object), true);
    %this.w00t = 0;
    %op = nameToID("MissionGroup\\ControlShack\\LockdownPanel");
    %op.w00t = 0; // Reset the other panel :D
  } else schedule("if ("@%this@".w00t=="@%this.w00t@")"@%this@".w00t=0;", 3);
}



